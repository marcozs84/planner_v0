{
	"name": "WebWorkPlan",
	"version": "1.0.0",
	"manifest_version": 2,
	"description": "Useful extension which allows you to organize your team tasks in a week view calendar",
	"background_page": "background.html",
	"omnibox": { "keyword" : "webworkplan" },
	"icons": {
		"16": "imgs/logo-16.png",
		"48": "imgs/logo-48.png",
		"128": "imgs/logo-128.png"
	},
	"permissions": [
		"tabs",
		"notifications",
		"<all_urls>"
	],
	"browser_action": {
		"default_title": "WebWorkPlan",
		"default_icon": "imgs/logo-16.png",
		"popup": "manager.html"
	}
	
}