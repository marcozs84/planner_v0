{
	"name": "WebWorkPlan",
	"version": "1.0.0",
	"description": "Useful extension which allows you to organize your team tasks in a week view calendar",
	"omnibox": { "keyword" : "webworkplan" },
	"icons": {
		"16": "images/logo-16.png",
		"48": "images/logo-48.png",
        "128": "images/logo-128.png"
	},
	"permissions": [
		"tabs",
		"notifications",
		"<all_urls>"
	],
	"default_locale" : "en",
	"options_page": "options.html",
	"browser_action": {
		"default_title": "WebWorkPlan",
		"default_icon": "images/logo-19.png",
		"popup": "manager.html"
	}
	
}